From 45090f7df84dcf749796c609da7a37e4bf3b598b Mon Sep 17 00:00:00 2001
From: Neil Deshpande <neildesh@gmail.com>
Date: Wed, 22 Oct 2025 09:36:38 +0000
Subject: [PATCH] Updating Cargo toml and lock for Rust edition 2021

---
 Cargo.lock | 2 +-
 Cargo.toml | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/Cargo.lock b/Cargo.lock
index 4435b8b..97a944e 100644
--- a/Cargo.lock
+++ b/Cargo.lock
@@ -1,6 +1,6 @@
 # This file is automatically @generated by Cargo.
 # It is not intended for manual editing.
-version = 4
+version = 3
 
 [[package]]
 name = "helloworld"
diff --git a/Cargo.toml b/Cargo.toml
index 448daef..384b4e3 100644
--- a/Cargo.toml
+++ b/Cargo.toml
@@ -1,6 +1,6 @@
 [package]
 name = "helloworld"
 version = "0.1.0"
-edition = "2024"
+edition = "2021"
 
 [dependencies]
